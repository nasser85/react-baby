{
  "name": "react-baby",
  "version": "1.2.0",
  "description": "A command line tool for generating a lightweight, full stack react application.",
  "preferGlobal": "true",
  "bin": {
    "react-baby": "bin/index.js"
  },
  "main": "bin/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir bin",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "baby",
    "boilerplate",
    "create",
    "app"
  ],
  "author": "Nasser Abouelazm",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.1",
    "chalk": "^2.4.1",
    "file-system": "^2.2.2",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "ava": "^0.21.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 10"
            ],
            "node": "current"
          }
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
